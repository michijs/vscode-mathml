{
  "name": "@michijs/vscode-mathml",
  "displayName": "MathML",
  "description": "MathML extension for VS Code",
  "author": "Lucas M. Segurado",
  "icon": "media/mathml.png",
  "version": "1.1.0",
  "publisher": "michijs",
  "files": [
    "generated/"
  ],
  "exports": {
    "./mathml.json": "./generated/mathml.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/michijs/vscode-mathml.git"
  },
  "galleryBanner": {
    "color": "#154db5",
    "theme": "dark"
  },
  "sponsor": {
    "url": "https://github.com/sponsors/michijs"
  },
  "license": "MIT",
  "engines": {
    "vscode": "^1.38.0"
  },
  "scripts": {
    "generate": "bun run ./tasks/generateTypes.ts",
    "test-tsc": "tsc",
    "package": "change-package-name vscode-mathml && vsce package && change-package-name @michijs/vscode-mathml",
    "publish-vsce": "vsce publish"
  },
  "keywords": [
    "math",
    "mathml",
    "html",
    "typescript"
  ],
  "devDependencies": {
    "@mdn/browser-compat-data": "7.1.23",
    "@michijs/shared-configs": "0.0.30",
    "@vscode/vsce": "3.7.1",
    "@vscode/web-custom-data": "0.6.1",
    "bun-types": "1.3.3",
    "change-package-name": "1.0.5",
    "compute-baseline": "0.4.0",
    "typescript": "5.9.3",
    "vscode-html-languageservice": "5.6.0"
  },
  "contributes": {
    "html": {
      "customData": [
        "./generated/mathml.json"
      ]
    }
  }
}
